<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mode PUBLIC
          "-//NetBeans//DTD Mode Properties 2.0//EN"
          "http://www.netbeans.org/dtds/mode-properties2_0.dtd">

<!-- window position similar to inspector in IDE -->
<mode version="2.0">
    <name unique="satellite" />
    <kind type="view" />
    <state type="joined" />
    <constraints>
        <path orientation="horizontal" number="20" weight="0.2" />
        <path orientation="vertical" number="80" weight="0.2" />
    </constraints>
    <relative-bounds x="10" y="22" width="32" height="20" />
    <empty-behavior permanent="true" />
</mode>
